{
  "name": "minecraft-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn build",
    "start": "pm2 start ./dist/ecosystem.config.js",
    "stop": "pm2 stop ./dist/ecosystem.config.js",
    "delete": "pm2 delete ./dist/ecosystem.config.js",
    "logs": "pm2 logs Scripticus",
    "dev": "yarn clean && tsc --project tsconfig.json && node .",
    "prebuild": "yarn clean",
    "build": "tsc --project tsconfig.production.json",
    "clean": "rimraf dist/"
  },
  "dependencies": {
    "discord.js": "^13.4.0",
    "dotenv": "^10.0.0",
    "mineflayer": "^3.13.1",
    "pm2": "^5.1.2",
    "prismarine-viewer": "^1.21.0",
    "typed-emitter": "^1.4.0"
  },
  "devDependencies": {
    "rimraf": "3.0.2",
    "typescript": "4.5.4"
  }
}
